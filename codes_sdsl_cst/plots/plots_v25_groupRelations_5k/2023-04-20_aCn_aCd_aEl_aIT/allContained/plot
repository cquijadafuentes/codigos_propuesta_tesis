red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]
set ylabel "Tiempo [us]"
set xlabel "Rutas"



set output 'tiempo_allContained_random.png'
set title 'Tiempo en Operación AllContained [d:trips - q:random [50] - s:compostela]'
plot 'data_trips1+_random_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_random_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_random_allContained' using 5 title 'NaivePC_{1+}' with linespoints pt 4 lc rgb green,   \
     'data_trips20+_random_allContained' using 5 title 'NaivePC_{20+}' with linespoints pt 5 lc rgb green,   \
     'data_trips1+_random_allContained' using 6 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_random_allContained' using 6 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allContained_largests.png'
set title 'Tiempo en Operación AllContained [d:trips - q:largests [50] - s:compostela]'
plot 'data_trips1+_largests_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_largests_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_largests_allContained' using 5 title 'NaivePC_{1+}' with linespoints pt 4 lc rgb green,   \
     'data_trips20+_largests_allContained' using 5 title 'NaivePC_{20+}' with linespoints pt 5 lc rgb green,   \
     'data_trips1+_largests_allContained' using 6 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_largests_allContained' using 6 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allContained_shortests.png'
set title 'Tiempo en Operación AllContained [d:trips - q:shortests [50] - s:compostela]'
plot 'data_trips1+_shortests_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_shortests_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_shortests_allContained' using 5 title 'NaivePC_{1+}' with linespoints pt 4 lc rgb green,   \
     'data_trips20+_shortests_allContained' using 5 title 'NaivePC_{20+}' with linespoints pt 5 lc rgb green,   \
     'data_trips1+_shortests_allContained' using 6 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_shortests_allContained' using 6 title 'GST_{20+}' with linespoints pt 5 lc rgb blue
