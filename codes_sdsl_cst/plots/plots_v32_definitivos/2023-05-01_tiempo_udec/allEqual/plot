red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]
set ylabel "Tiempo [us]"
set xlabel "Rutas"



set output 'tiempo_allEqual_random.png'
set title 'Tiempo en Operación AllEqual [d:trips - q:random [2000-1r] - s:udec]'
plot 'data_trips1+_random_allEqual' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_random_allEqual' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_random_allEqual' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_random_allEqual' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allEqual_largests.png'
set title 'Tiempo en Operación AllEqual [d:trips - q:largests [100-1r] - s:udec]'
plot 'data_trips1+_largests_allEqual' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_largests_allEqual' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_largests_allEqual' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_largests_allEqual' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allEqual_shortests.png'
set title 'Tiempo en Operación AllEqual [d:trips - q:shortests [100-1r] - s:udec]'
plot 'data_trips1+_shortests_allEqual' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_shortests_allEqual' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_shortests_allEqual' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_shortests_allEqual' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue
