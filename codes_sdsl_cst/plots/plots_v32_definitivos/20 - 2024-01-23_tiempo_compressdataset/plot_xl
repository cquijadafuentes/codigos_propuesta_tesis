red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400
set logscale y
set xrange [0.8:3.2]

set ylabel "Time [us(log)]"


set xlabel "Operation"
set output 'fig_tiempo_allTopoRels_compress.png'
plot 'data_relations' using 5:xtic(2) title 'Baseline' with linespoints pt 4 lc rgb red,   \
     '' using 6 title 'TRGST' with linespoints pt 4 lc rgb blue



set xrange [0.8:4.2]
set xlabel "k"
set output 'fig_tiempo_allIntersectPP_k_compress.png'
#set title 'Tiempo en Operaci√≥n AllIntersectPP_k [d:tripsXL - q:largest [200-1r] - s:tumbes]'
plot 'data_intersect' using 4:xtic(9) title 'Baseline' with line lt 1 lc rgb red,   \
     '' using 5 title 'TRGST' with linespoints pt 5 lc rgb blue