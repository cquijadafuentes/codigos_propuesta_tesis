red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]

set ylabel "Time [us]"
set xlabel "Number of Stops"
set logscale y



set output 'tiempo_allContain_xl_stops_variables.png'
#set title 'Time en Operación AllContain [d:tripsXL - q:random [1000-3r] - s:tumbes]'
plot 'data_tripsxl_random_allContain' using 5:xtic(3) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 6 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set output 'tiempo_allContained_xl_stops_variables.png'
#set title 'Time en Operación AllContained [d:tripsXL - q:random [1000-3r] - s:tumbes]'
plot 'data_tripsxl_random_allContained' using 5:xtic(3) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 6 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set output 'tiempo_allEqual_xl_stops_variables.png'
#set title 'Time en Operación AllEqual [d:tripsXL - q:random [1000-3r] - s:tumbes]'
plot 'data_tripsxl_random_allEqual' using 5:xtic(3) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 6 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set output 'tiempo_allIntersect_xl_stops_variables.png'
#set title 'Time en Operación AllIntersect [d:tripsXL - q:random [1000-3r] - s:tumbes]'
plot 'data_tripsxl_random_allIntersect_k1' using 5:xtic(3) title 'Baseline' with linespoints pt 5 lc rgb gold,   \
     '' using 6 title 'TRGST_{1}' with linespoints pt 4 lc rgb red,   \
     'data_tripsxl_random_allIntersect_k5' using 6:xtic(3) title 'TRGST_{5}' with linespoints pt 4 lc rgb green,   \
     'data_tripsxl_random_allIntersect_k15' using 6:xtic(3) title 'TRGST_{15}' with linespoints pt 4 lc rgb blue



set ylabel "Cantidad de resultados"
set xlabel "Cantidad de stops"
set output 'cantidad_resultados_stops_variables.png'
set title 'Resultados en Operación AllContain [d:tripsXL - s:tumbes]'
plot 'data_tripsxl_random_allContain' using 8:xtic(3) title 'allContain' with linespoints pt 4 lc rgb red, \
     'data_tripsxl_random_allContained' using 8:xtic(3) title 'allContained' with linespoints pt 4 lc rgb green, \
     'data_tripsxl_random_allEqual' using 8:xtic(3) title 'allEqual' with linespoints pt 4 lc rgb blue

