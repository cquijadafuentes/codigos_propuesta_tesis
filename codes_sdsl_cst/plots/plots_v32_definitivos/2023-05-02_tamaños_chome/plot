red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; darkgreen = "#006400"; darkred = "#8b0000"; gold = "#ffd700";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.8:4.2]

set output 'img_size_gst_trips.png'
set title 'Tamaño GST'
set ylabel "Tamaño [B]"
set xlabel "Rutas"
plot 'data' using 3:xtic(1) title 'cst_{sada}' with linespoints lt 2 pt 3 lc rgb red,   \
     'data' using 6 title 'gstMFSbv' with linespoints lt 2 pt 6 lc rgb darkred,   \
     'data' using 5 title 'mapN2R' with linespoints lt 2 pt 5 lc rgb darkgreen,   \
     'data' using 4 title 'mapR2N' with linespoints lt 2 pt 4 lc rgb green,   \
     'data' using 7 title 'gstMNodos' with linespoints lt 2 pt 7 lc rgb blue,   \
     'data' using 8 title 'gstMRamas' with linespoints lt 2 pt 7 lc rgb skyblue,   \
     'data' using 9 title 'GST' with linespoints lt 2 pt 7 lc rgb black,   \
     'data' using 10 title 'Naive' with linespoints lt 2 pt 7 lc rgb gold




set logscale y
set output 'img_size_gst_trips_log.png'
set ylabel "Tamaño [MB(log)]"
plot 'data' using 3:xtic(1) title 'cst_{sada}' with linespoints lt 2 pt 3 lc rgb red,   \
     'data' using 6 title 'gstMFSbv' with linespoints lt 2 pt 6 lc rgb darkred,   \
     'data' using 5 title 'mapN2R' with linespoints lt 2 pt 5 lc rgb darkgreen,   \
     'data' using 4 title 'mapR2N' with linespoints lt 2 pt 4 lc rgb green,   \
     'data' using 7 title 'gstMNodos' with linespoints lt 2 pt 7 lc rgb blue,   \
     'data' using 8 title 'gstMRamas' with linespoints lt 2 pt 7 lc rgb skyblue,   \
     'data' using 9 title 'GST' with linespoints lt 2 pt 7 lc rgb black,   \
     'data' using 10 title 'Naive' with linespoints lt 2 pt 7 lc rgb gold
