red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]
set ylabel "Tiempo [us]"
set xlabel "Rutas"



set output 'tiempo_allContained_5k-50k_random.png'
set title 'Tiempo en Operaci贸n AllContained [d:trips - q:random [1000-3r] - s:tumbes]'
plot 'data_trips1+_random_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_random_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_random_allContained' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_random_allContained' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allContained_5k-50k_largests.png'
set title 'Tiempo en Operaci贸n AllContained [d:trips - q:largests [500-3r] - s:tumbes]'
plot 'data_trips1+_largests_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_largests_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_largests_allContained' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_largests_allContained' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allContained_5k-50k_shortests.png'
set title 'Tiempo en Operaci贸n AllContained [d:trips - q:shortests [500-3r] - s:tumbes]'
plot 'data_trips1+_shortests_allContained' using 4:xtic(2) title 'Naive_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_shortests_allContained' using 4:xtic(2) title 'Naive_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_shortests_allContained' using 5 title 'GST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_shortests_allContained' using 5 title 'GST_{20+}' with linespoints pt 5 lc rgb blue




set logscale y
set ylabel "Cantidad de resultados"
set xlabel "Cantidad de rutas"
set output 'cantidad_resultados_allContained_5k-50k.png'
set title 'Resultados en Operaci贸n AllContained [d:trips - s:tumbes]'
plot 'data_trips1+_random_allContained' using 8:xtic(2) title 'random_{1+}' with linespoints pt 4 lc rgb darkgreen,   \
     'data_trips20+_random_allContained' using 8 title 'random_{20+}' with linespoints pt 5 lc rgb darkgreen,   \
     'data_trips1+_largests_allContained' using 8 title 'largest_{1+}' with linespoints pt 4 lc rgb skyblue,   \
     'data_trips20+_largests_allContained' using 8 title 'largest_{20+}' with linespoints pt 5 lc rgb skyblue,   \
     'data_trips1+_shortests_allContained' using 8 title 'shortest_{1+}' with linespoints pt 4 lc rgb gold,   \
