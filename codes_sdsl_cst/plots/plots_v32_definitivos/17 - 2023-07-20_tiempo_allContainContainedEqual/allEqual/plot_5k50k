red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]
set ylabel "Time [us]"
set xlabel "Trajectories"



set output 'tiempo_allEqual_5k-50k_random.png'
#set title 'Time en Operaci贸n allEqual [d:trips - q:random [1000-3r] - s:tumbes]'
plot 'data_trips1+_random_allEqual' using 4:xtic(2) title 'Baseline_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_random_allEqual' using 4:xtic(2) title 'Baseline_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_random_allEqual' using 5 title 'TRGST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_random_allEqual' using 5 title 'TRGST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allEqual_5k-50k_largests.png'
#set title 'Time en Operaci贸n allEqual [d:trips - q:largests [500-3r] - s:tumbes]'
plot 'data_trips1+_largests_allEqual' using 4:xtic(2) title 'Baseline_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_largests_allEqual' using 4:xtic(2) title 'Baseline_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_largests_allEqual' using 5 title 'TRGST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_largests_allEqual' using 5 title 'TRGST_{20+}' with linespoints pt 5 lc rgb blue



set output 'tiempo_allEqual_5k-50k_shortests.png'
#set title 'Time en Operaci贸n allEqual [d:trips - q:shortests [500-3r] - s:tumbes]'
plot 'data_trips1+_shortests_allEqual' using 4:xtic(2) title 'Baseline_{1+}' with linespoints pt 4 lc rgb red,   \
     'data_trips20+_shortests_allEqual' using 4:xtic(2) title 'Baseline_{20+}' with linespoints pt 5 lc rgb red,   \
     'data_trips1+_shortests_allEqual' using 5 title 'TRGST_{1+}' with linespoints pt 4 lc rgb blue,   \
     'data_trips20+_shortests_allEqual' using 5 title 'TRGST_{20+}' with linespoints pt 5 lc rgb blue




set logscale y
set ylabel "Cantidad de resultados"
set xlabel "Cantidad de rutas"
set output 'cantidad_resultados_allEqual_5k-50k.png'
set title 'Resultados en Operaci贸n allEqual [d:trips - s:tumbes]'
plot 'data_trips1+_random_allEqual' using 8:xtic(2) title 'random_{1+}' with linespoints pt 4 lc rgb darkgreen,   \
     'data_trips20+_random_allEqual' using 8 title 'random_{20+}' with linespoints pt 5 lc rgb darkgreen,   \
     'data_trips1+_largests_allEqual' using 8 title 'largest_{1+}' with linespoints pt 4 lc rgb skyblue,   \
     'data_trips20+_largests_allEqual' using 8 title 'largest_{20+}' with linespoints pt 5 lc rgb skyblue,   \
     'data_trips1+_shortests_allEqual' using 8 title 'shortest_{1+}' with linespoints pt 4 lc rgb gold,   \
     'data_trips20+_shortests_allEqual' using 8 title 'shortest_{20+}' with linespoints pt 5 lc rgb gold


