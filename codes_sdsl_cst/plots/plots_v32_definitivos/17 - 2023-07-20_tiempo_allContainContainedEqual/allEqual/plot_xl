red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:7.5]

set ylabel "Time [us]"
set xlabel "Trajectories"
set logscale y



set output 'tiempo_allEqual_xl_random.png'
#set title 'Time en Operaci贸n AllEqual [d:tripsXL - q:random [1000-3r] - s:tumbes]'
plot 'data_tripsxl_random_allEqual' using 4:xtic(2) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 5 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set output 'tiempo_allEqual_xl_largests.png'
#set title 'Time en Operaci贸n AllEqual [d:tripsXL - q:largests [500-3r] - s:tumbes]'
plot 'data_tripsxl_largests_allEqual' using 4:xtic(2) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 5 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set output 'tiempo_allEqual_xl_shortests.png'
#set title 'Time en Operaci贸n AllEqual [d:tripsXL - q:shortests [500-3r] - s:tumbes]'
plot 'data_tripsxl_shortests_allEqual' using 4:xtic(2) title 'Baseline_{XL}' with linespoints pt 4 lc rgb red,   \
     '' using 5 title 'TRGST_{XL}' with linespoints pt 4 lc rgb blue



set ylabel "Cantidad de resultados"
set xlabel "Cantidad de rutas"
set output 'cantidad_resultados_allEqual_xl.png'
set title 'Resultados en Operaci贸n AllContain [d:tripsXL - s:tumbes]'
plot 'data_tripsxl_random_allEqual' using 8:xtic(2) title 'random' with linespoints pt 4 lc rgb darkgreen,   \
     'data_tripsxl_largests_allEqual' using 8 title 'largest' with linespoints pt 4 lc rgb skyblue,   \
     'data_tripsxl_shortests_allEqual' using 8 title 'shortest' with linespoints pt 4 lc rgb gold



