red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; gold = "#ffd700"; darkgreen = "#006400";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:8.5]
set ylabel "Tiempo [us]"
set xlabel "Rutas"



set output 'tiempo_allContain_xl_random.png'
set title 'Tiempo en Operaci贸n AllContain [d:trips_{XL} - q:random [1000-3r] - s:tumbes]'
plot 'data_random_allContain' using 4:xtic(2) title 'Naive' with linespoints pt 4 lc rgb red,   \
     'data_random_allContain' using 5 title 'GST' with linespoints pt 4 lc rgb blue



set output 'tiempo_allContain_xl_largests.png'
set title 'Tiempo en Operaci贸n AllContain [d:trips_{XL} - q:largests [5-3r] - s:tumbes]'
plot 'data_largests_allContain' using 4:xtic(2) title 'Naive' with linespoints pt 4 lc rgb red,   \
     'data_largests_allContain' using 5 title 'GST' with linespoints pt 4 lc rgb blue



set output 'tiempo_allContain_xl_shortests.png'
set title 'Tiempo en Operaci贸n AllContain [d:trips_{XL} - q:shortests [5-3r] - s:tumbes]'
plot 'data_shortests_allContain' using 4:xtic(2) title 'Naive' with linespoints pt 4 lc rgb red,   \
     'data_shortests_allContain' using 5 title 'GST' with linespoints pt 4 lc rgb blue



set logscale y
set xrange [0.5:8.5]
set ylabel "Cantidad de resultados"
set xlabel "Cantidad de rutas"
set output 'cantidad_resultados_allContain_xl.png'
set title 'Resultados en Operaci贸n AllContain [d:trips_{XL} - s:tumbes]'
plot 'data_random_allContain' using 8:xtic(2) title 'random' with linespoints pt 4 lc rgb darkgreen,   \
     'data_largests_allContain' using 8 title 'largest' with linespoints pt 4 lc rgb skyblue,   \
     'data_shortests_allContain' using 8 title 'shortest' with linespoints pt 4 lc rgb gold



