red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; darkgreen = "#006400"; darkred = "#8b0000";
set grid ytics
set key out horiz
set terminal png size 800,400

set xrange [0.5:5.5]
set ylabel "Tiempo [us]"
set xlabel "Rutas"



set output 'tiempo_allContained_random_trips1+.png'
set title 'Tiempo en Operación AllContained [d:trips1+ - q:random[50] - s:compostela]'
plot 'data_trips1+_random' using 3:xtic(1) title 'v1' with linespoints pt 4 lc rgb red,   \
     '' using 4 title 'v2' with linespoints pt 4 lc rgb green,   \
     '' using 5 title 'v3' with linespoints pt 5 lc rgb blue,   \
     '' using 6 title 'v4' with linespoints pt 4 lc rgb skyblue,   \
     '' using 7 title 'v5' with linespoints pt 4 lc rgb darkgreen,   \
     '' using 8 title 'v6' with linespoints pt 5 lc rgb darkred



